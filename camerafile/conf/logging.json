{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "console": {
      "format": "[%(asctime)s] %(message)s",
      "datefmt": "%H:%M:%S"
    },
    "simple": {
      "format": "[%(asctime)s] %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "console",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "console"
    ]
  }
}